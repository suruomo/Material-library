<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.suruomo.material.dao.DisplacementResultMapper" >
  <resultMap id="BaseResultMap" type="com.suruomo.material.pojo.DisplacementResult" >
    <result column="ID" property="id" jdbcType="DECIMAL" />
    <result column="ANALYSIS_ID" property="analysisId" jdbcType="DECIMAL" />
    <result column="POINT_ID" property="pointId" jdbcType="DECIMAL" />
    <result column="T1" property="t1" jdbcType="VARCHAR" />
    <result column="T2" property="t2" jdbcType="VARCHAR" />
    <result column="T3" property="t3" jdbcType="VARCHAR" />
    <result column="R1" property="r1" jdbcType="VARCHAR" />
    <result column="R2" property="r2" jdbcType="VARCHAR" />
    <result column="R3" property="r3" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.suruomo.material.pojo.DisplacementResult" >
    insert into DISPLACEMENT_RESULT (ID, ANALYSIS_ID, POINT_ID, 
      T1, T2, T3, R1, 
      R2, R3)
    values (#{id,jdbcType=DECIMAL}, #{analysisId,jdbcType=DECIMAL}, #{pointId,jdbcType=DECIMAL}, 
      #{t1,jdbcType=VARCHAR}, #{t2,jdbcType=VARCHAR}, #{t3,jdbcType=VARCHAR}, #{r1,jdbcType=VARCHAR}, 
      #{r2,jdbcType=VARCHAR}, #{r3,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.suruomo.material.pojo.DisplacementResult" >
    insert into DISPLACEMENT_RESULT
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        ID,
      </if>
      <if test="analysisId != null" >
        ANALYSIS_ID,
      </if>
      <if test="pointId != null" >
        POINT_ID,
      </if>
      <if test="t1 != null" >
        T1,
      </if>
      <if test="t2 != null" >
        T2,
      </if>
      <if test="t3 != null" >
        T3,
      </if>
      <if test="r1 != null" >
        R1,
      </if>
      <if test="r2 != null" >
        R2,
      </if>
      <if test="r3 != null" >
        R3,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=DECIMAL},
      </if>
      <if test="analysisId != null" >
        #{analysisId,jdbcType=DECIMAL},
      </if>
      <if test="pointId != null" >
        #{pointId,jdbcType=DECIMAL},
      </if>
      <if test="t1 != null" >
        #{t1,jdbcType=VARCHAR},
      </if>
      <if test="t2 != null" >
        #{t2,jdbcType=VARCHAR},
      </if>
      <if test="t3 != null" >
        #{t3,jdbcType=VARCHAR},
      </if>
      <if test="r1 != null" >
        #{r1,jdbcType=VARCHAR},
      </if>
      <if test="r2 != null" >
        #{r2,jdbcType=VARCHAR},
      </if>
      <if test="r3 != null" >
        #{r3,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
    <select id="getAll" resultType="com.suruomo.material.pojo.DisplacementResult">
       select * from (
    <![CDATA[
    select rownum rn,a.* from (select * from "METAL"."DISPLACEMENT_RESULT" where ANALYSIS_ID=#{param1}) a where rownum <= #{end}
    ]]>
    )
    </select>
</mapper>